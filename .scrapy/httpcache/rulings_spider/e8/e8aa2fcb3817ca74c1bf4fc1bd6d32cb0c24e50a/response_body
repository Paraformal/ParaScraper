    <script type="text/javascript">
        var currentIndex = 0;
        var max = parseInt("7");
        function ShowYearRow(index) {
            document.getElementById("btnNext").disabled = "";
            document.getElementById("btnLast").disabled = "";
            document.getElementById("btnFirst").disabled = "";
            document.getElementById("btnPrevious").disabled = "";

            var trs = document.getElementsByTagName("tr");
            for (var i = 0; i < trs.length; i++) {
                if (trs[i].id.indexOf("trYear_") >= 0) {
                    trs[i].style.display = "none";
                }
            }

            document.getElementById("trYear_" + index).style.display = "block";
            document.getElementById("tdPaging").innerHTML = "صفحة <span style='color: #D9251C; Font-weight:bold'> " + (currentIndex + 1) + "</span> من " + max;

            if (currentIndex == 0) {
                document.getElementById("btnFirst").disabled = "disabled";
                document.getElementById("btnPrevious").disabled = "disabled";
                document.getElementById("btnNext").disabled = "";
                document.getElementById("btnLast").disabled = "";
            }

            if (currentIndex == max - 1) {
                document.getElementById("btnFirst").disabled = "";
                document.getElementById("btnPrevious").disabled = "";
                document.getElementById("btnNext").disabled = "disabled";
                document.getElementById("btnLast").disabled = "disabled";
            }
        }
        //ShowYearRow(currentIndex);

        function ShowNext() {
            if (currentIndex < max - 1) {
                currentIndex = currentIndex + 1;
            }
            ShowYearRow(currentIndex);
        }

        function ShowPrevious() {
            if (currentIndex - 1 >= 0) {
                currentIndex = currentIndex - 1;
            }
            ShowYearRow(currentIndex);
        }

        function ShowFirst() {
            currentIndex = 0;
            ShowYearRow(0);
        }

        function ShowLast() {
            currentIndex = max - 1;
            ShowYearRow(currentIndex);
        }
    </script>
